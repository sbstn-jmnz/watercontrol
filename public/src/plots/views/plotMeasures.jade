.row
  div(ncy-breadcrumb)
 
  .plots-list.col-sm-8.col-lg-6
    table.table.table-hover.table-striped
      thead
        tr
          th.col-xs-3 Valor Medido
          th.col-xs-2 Comentario
          th.col-xs-3 Usuario Medicion
          th.col-xs-4 Estado

        tbody
          tr(ng-repeat="measure in plotMeasures.plotMeasures.measures track by measure.id")
            td(ng-bind="measure.value")
            td(ng-bind="measure.comment")
            td(ng-bind="measure.user")
            td(ng-bind="measure.status")
            
